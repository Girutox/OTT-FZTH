{
  "name": "fzth",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "webos-generate": "ares-generate stv/webos",
    "webos-prepare": "ng build && del stv\\webos\\static && xcopy dist\\fzth\\ stv\\webos\\static /e /h",
    "webos-build": "xcopy stv\\webos\\static stv\\webos /e /h && ares-package --no-minify stv/webos -o stv/webos/build",
    "webos-deploy": "ares-install stv/webos/build/com.domain.app_1.0.0_all.ipk",
    "webos-uninstall": "ares-install --remove com.domain.app",
    "webos-inspect": "ares-inspect com.domain.app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@angular/youtube-player": "^15.1.4",
    "@videogular/ngx-videogular": "^7.0.1",
    "firebase": "^9.17.1",
    "js-spatial-navigation": "^1.0.1",
    "rxjs": "~7.8.0",
    "spatial-navigation-polyfill": "^1.3.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.4",
    "@angular/cli": "~15.1.4",
    "@angular/compiler-cli": "^15.1.0",
    "@types/core-js": "^2.5.5",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
